<h1>オーナー 新規登録</h1>
<%= form_for :owner, url: { controller: 'owners', action: 'create' } do |f| %>
  <%= f.label :name, "名前" %>
  <%= f.text_field :name %><br/>
  <%= f.label :email, "Eメール" %>
  <%= f.text_field :email%><br/>
  <%= f.label :password, "パスワード" %>
  <%= f.text_field :password %><br/>
  <%= f.label :password_confirmation, "パスワードの確認" %>
  <%= f.text_field :password_confirmation %><br/>
  <%= f.submit "登録" %><br/>

  <%= link_to "すでにアカウントをお持ちの方はこちら", o_login_path %>
<% end %>